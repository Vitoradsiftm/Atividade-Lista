def insertionSort(lista):
    n = len(lista)
    iteracoes = 0
    print("Vetor inicial: ", lista)
    
    for i in range(1, n):
        chave = lista[i]
        j = i - 1
        houve_troca = False
        
        while j >= 0 and lista[j] > chave:
            lista[j + 1] = lista[j]
            j -= 1
            iteracoes += 1
            houve_troca = True
        lista[j + 1] = chave
        
        print(f"Iteração {i}: {lista}")
        
        if not houve_troca:
            break
    
    print("Vetor final: ", lista)
    return iteracoes
